{
    "name": "apacheborys/symfony-keycloak-bundle",
    "description": "Symfony bundle that bridges Keycloak PHP client into Symfony applications.",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "php": "^8.3",
        "apacheborys/keycloak-php-client": "^0.0.7",
        "symfony/config": "^7.4",
        "symfony/dependency-injection": "^7.4",
        "symfony/framework-bundle": "^7.4",
        "symfony/http-kernel": "^7.4"
    },
    "autoload": {
        "psr-4": {
            "Apacheborys\\SymfonyKeycloakBridgeBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Apacheborys\\SymfonyKeycloakBridgeBundle\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "nyholm/psr7": "^1.8",
        "phpunit/phpunit": "^11.0",
        "phpstan/phpstan": "^1.11",
        "rector/rector": "^1.2",
        "squizlabs/php_codesniffer": "^3.10"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/apacheborys/keycloak-php-client"
        }
    ],
    "prefer-stable": true,
    "scripts": {
        "phpcs": "phpcs --standard=phpcs.xml",
        "phpstan": "phpstan analyse -c phpstan.neon",
        "rector": "rector process --dry-run",
        "test": "phpunit",
        "check": [
            "@phpcs",
            "@phpstan",
            "@rector",
            "@test"
        ]
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
